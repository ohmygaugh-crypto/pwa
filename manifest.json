{
    "name": "Foodhobo PWA",
    "short_name": "Foodhobo",
    "description": "Your app's brief description here.",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#fff",
    "theme_color": "#673ab7",
    "orientation": "any",
    "scope": "/",
    "icons": [
      {
        "src": "react-pwa-ts/public/logo192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "react-pwa-ts/public/logo512.png",
        "sizes": "512x512",
        "type": "image/png"
      }
    ],
    "shortcuts": [
      {
        "name": "Shortcut Name",
        "short_name": "ShortcutShortName",
        "description": "Description of the shortcut.",
        "url": "/path-to-shortcut-url",
        "icons": [
          {
            "src": "path-to-shortcut-icon.png",
            "sizes": "96x96",
            "type": "image/png"
          }
        ]
      }
      // ... add more shortcuts as needed
    ],
    "share_target": {
      "action": "/share-target/",
      "method": "GET",
      "enctype": "application/x-www-form-urlencoded",
      "params": {
        "title": "title",
        "text": "text",
        "url": "url"
      }
    },
    "related_applications": [
      {
        "platform": "play",
        "url": "https://play.google.com/store/apps/details?id=your.package.name"
      }
      // ... add other platforms if you have corresponding native apps
    ],
    "prefer_related_applications": false
}
